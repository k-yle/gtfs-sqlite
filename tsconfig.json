{
  "extends": "eslint-config-kyle/tsconfig",
  "compilerOptions": {
    "target": "es2020",
    "lib": ["esnext", "dom", "webworker"],
    "module": "esnext",
    "moduleResolution": "bundler",
    "moduleDetection": "force",
    "types": ["wicg-file-system-access", "wicg-task-scheduling", "vite/client"],
    "noUncheckedSideEffectImports": true
  },
  "include": ["src"]
}
